&dsi_sharp_4k_dsc_video {
	qcom,mdss-dsi-panel-timings = [00 36 09 0c 15 16 09 0d 0a 03 04 00];
	qcom,mdss-dsi-t-clk-post = <0x0d>;
	qcom,mdss-dsi-t-clk-pre = <0x25>;
};

&dsi_sharp_4k_dsc_cmd {
